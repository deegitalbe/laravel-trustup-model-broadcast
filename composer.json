{
    "name": "deegitalbe/laravel-trustup-model-broadcast",
    "description": "Basic versioned php package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^1.3",
        "pusher/pusher-php-server": "^7.2",
        "deegitalbe/laravel-trustup-io-websocket": "^0.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegitalbe\\LaravelTrustupModelBroadcast\\": "src"
