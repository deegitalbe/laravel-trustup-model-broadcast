    "autoload-dev": {
        "psr-4": {
            "Deegitalbe\\LaravelTrustupModelBroadcast\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegitalbe\\LaravelTrustupModelBroadcast\\Providers\\LaravelTrustupModelBroadcastServiceProvider"
            ]
        }
    }
